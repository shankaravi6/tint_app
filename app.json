    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tint.react"
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "f492d27d-40b3-4790-b133-f32dac3127e3"
      }
    }
  }
}
