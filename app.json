{
  "expo": {
    "name": "Tint",
    "slug": "Tint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "splash": {
      "image": "./assets/sp.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
